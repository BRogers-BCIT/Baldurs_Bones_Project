<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox id="QuitMenu" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <Region fx:id="TopImageArea" prefHeight="100.0" />
   <Label id="QuitLabel" contentDisplay="TEXT_ONLY" ellipsisString="" graphicTextGap="0.0" text="Are you sure you want to quit?" textAlignment="CENTER" textOverrun="CLIP" VBox.vgrow="NEVER">
      <font>
         <Font size="24.0" />
      </font>
      <padding>
         <Insets bottom="20.0" left="20.0" right="20.0" top="40.0" />
      </padding>
   </Label>
   <HBox id="QuitButtonsContainer" alignment="CENTER" VBox.vgrow="ALWAYS">
      <padding>
         <Insets bottom="20.0" />
      </padding>
      <Region HBox.hgrow="ALWAYS" />
      <Button id="CancelQuit" contentDisplay="TEXT_ONLY" ellipsisString="" graphicTextGap="0.0" mnemonicParsing="false" styleClass="QuitButton" text="Cancel Quit" textAlignment="CENTER" textOverrun="CLIP" />
      <Region HBox.hgrow="ALWAYS" />
      <Button id="MenuQuit" contentDisplay="TEXT_ONLY" ellipsisString="" graphicTextGap="0.0" mnemonicParsing="false" styleClass="QuitButton" text="Return to Menu" textAlignment="CENTER" textOverrun="CLIP" />
      <Region HBox.hgrow="ALWAYS" />
      <Button id="DesktopQuit" contentDisplay="TEXT_ONLY" ellipsisString="" graphicTextGap="0.0" mnemonicParsing="false" styleClass="QuitButton" text="Quit to Desktop" textAlignment="CENTER" textOverrun="CLIP" />
      <Region HBox.hgrow="ALWAYS" />
   </HBox>
</VBox>
