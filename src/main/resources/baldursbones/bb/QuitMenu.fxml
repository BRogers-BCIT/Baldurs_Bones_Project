<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<HBox xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Region fx:id="QuitMenuLeft" prefWidth="50.0" />
      <VBox id="QuitMenu" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity">
         <Region fx:id="QuitMenuHeader" prefHeight="50.0" />
         <HBox id="QuitTitleContainer" alignment="CENTER">
            <children>
               <Region fx:id="QuitTitleLeft" minWidth="30.0" />
               <Label id="QuitTitle" contentDisplay="TEXT_ONLY" ellipsisString="" graphicTextGap="0.0" text="Are you sure you want to quit?" textAlignment="CENTER" textOverrun="CLIP">
                  <font>
                     <Font size="24.0" />
                  </font>
                  <HBox.margin>
                     <Insets />
                  </HBox.margin>
                  <padding>
                     <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                  </padding>
               </Label>
               <Region fx:id="QuitTitleRight" minWidth="30.0" />
            </children>
            <VBox.margin>
               <Insets />
            </VBox.margin>
         </HBox>
         <HBox id="QuitButtonsContainer" alignment="CENTER" VBox.vgrow="ALWAYS">
            <Region HBox.hgrow="ALWAYS" />
            <Button id="CancelQuit" contentDisplay="TEXT_ONLY" ellipsisString="" graphicTextGap="0.0" mnemonicParsing="false" styleClass="QuitButton" text="Cancel Quit" textAlignment="CENTER" textOverrun="CLIP">
               <HBox.margin>
                  <Insets bottom="20.0" top="20.0" />
               </HBox.margin>
            </Button>
            <Region HBox.hgrow="ALWAYS" />
            <Button id="MenuQuit" contentDisplay="TEXT_ONLY" ellipsisString="" graphicTextGap="0.0" mnemonicParsing="false" styleClass="QuitButton" text="Return to Menu" textAlignment="CENTER" textOverrun="CLIP">
               <HBox.margin>
                  <Insets bottom="20.0" top="20.0" />
               </HBox.margin>
            </Button>
            <Region HBox.hgrow="ALWAYS" />
            <Button id="DesktopQuit" contentDisplay="TEXT_ONLY" ellipsisString="" graphicTextGap="0.0" mnemonicParsing="false" styleClass="QuitButton" text="Quit to Desktop" textAlignment="CENTER" textOverrun="CLIP">
               <HBox.margin>
                  <Insets bottom="20.0" top="20.0" />
               </HBox.margin>
            </Button>
            <Region HBox.hgrow="ALWAYS" />
         </HBox>
         <Region fx:id="QuitMenuFooter" prefHeight="50.0" />
      </VBox>
      <Region fx:id="QuitMenuRight" prefWidth="50.0" />
   </children>
</HBox>
